input ReviewCreateRequest{
    title: String! @NotBlank
    content: String! @NotBlank
    score: Int!
    status: Boolean!
    userId: String! @NotBlank
    itemId: String! @NotBlank
    images: [Upload!]!
}

input ReviewDeleteRequest{
    reviewId: String! @NotBlank
}

input ReviewSearchRequest{
    id: [String!]! @NotBlank
}

input ReviewUpdateRequest{
    reviewId: String! @NotBlank
    title: String! @NotBlank
    content: String! @NotBlank
    score: Int!
    status: Boolean!
    userId: String! @NotBlank
    itemId: String! @NotBlank
    originImages: [String]
    addImages: [Upload!]
    likeUsers: [String]
}

enum ReviewSearchType{
    # Review Id로 찾기
    REVIEW_ID
    # User Id로 찾기
    USER_ID
    # Item Id로 찾기
    ITEM_ID
    # User Id와 Item Id 두개로 찾기
    USER_AND_ITEM_ID
}

type ReviewResponse{
    reviewId: String
    title: String
    content: String
    score: Int
    status: Boolean
    userId: String
    itemId: String
    images: [String]
    likeUsers: [String]
}