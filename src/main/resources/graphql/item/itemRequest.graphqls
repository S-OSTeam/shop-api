input ItemRequest {
    categoryPublicId: String!
    title: String! @NotBlank
    content: String! @NotBlank
    summary: String! @NotBlank
    price: Int!
    status: Boolean!
    sellerId: String! @NotBlank
    stockCnt: Int!
    freeDelivery: Boolean!
    images: [ImageRequest]!
}

input ItemCategoryRequest {
    title: String! @NotBlank
    parentPublicId: String
}